{
  "name": "freshservice-user-search",
  "version": "1.0.0",
  "description": "Search for users and requesters in Freshservice",
  "type": "module",
  "scripts": {
    "test": "jest --coverage",
    "validate": "validate.bat",
    "pack": "validate.bat && copy build\\dist\\build.zip ."
  },
  "dependencies": {},
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/scripts/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "jsdom"
  }
} 